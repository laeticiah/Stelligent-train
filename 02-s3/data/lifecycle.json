{
    "Rules": [
      {
        "ID": "Move to Glacier after ninety days",
        "Prefix": "*",
        "Status": "Enabled",
        "Transition": {
          "Days": 90,
          "StorageClass": "GLACIER"
        }
      },
    
      {
        "ID": "Move to Infrequent Access after thirty days",
        "Prefix": "*",
        "Status": "Enabled",
        "Transition": {
          "Days": 30,
          "StorageClass": "INFREQUENT ACCESS"
      },

        "Transition": {
        "NoncurrentVersionTransition": {
        "NoncurrentDays": 7,
        "StorageClass": "INTELLIGENT_TIERING"
        },
        "NoncurrentVersionExpiration": {
        "NoncurrentDays": 7
        },
        "AbortIncompleteMultipartUpload": {
        "DaysAfterInitiation": 1
        }
    },  
    ]
  }